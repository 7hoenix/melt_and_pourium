<div class="container text-center">


  <br>

  <div class="row">
    <div class="col-md-offset-4 col-md-4 well" id="login_form">
      <h2>Login</h2>
      <%= bootstrap_form_for :sessions, url: login_path do |f| %>
        <%= f.text_field :email %>
        <%= f.password_field :password, icon: "lock" %>
        <%= f.submit "Login", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
